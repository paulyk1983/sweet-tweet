<div class='chart-container' ng-app="app">
  <div ng-controller="chartsCtrl" ng-init='hideChart()'>
    <h1>Sweet Tweet Charts</h1>
    <div class="btn-group" role="group" aria-label="..." style='margin-right:25px'>
      <button id='button' type="button" class="btn btn-default active" ng-click='showChartOne()'>Past 6 Months</button>
      <button type="button" class="btn btn-default" ng-click='showChartTwo()'>Past Year</button>
    </div>
    <%= high_chart("chart-1", @chart1) %>
    <%= high_chart("chart-2", @chart2) %> 
  </div>
</div>